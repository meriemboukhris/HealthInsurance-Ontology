                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :MonthlyRevenue ;
                         owl:hasValue "1600.0"^^xsd:double
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :Sector ;
                         owl:hasValue "Public"
                       ] .


